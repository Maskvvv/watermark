{
  "presets": [
    ["env", {
      "targets": { "browsers": ["last 2 versions", "IE >= 9"] },
      "modules": false,
      "useBuiltIns": "entry"
    }],
    "react"
  ],
  "plugins": [
    ["transform-runtime", { "polyfill": false, "regenerator": true }],
    "transform-decorators-legacy",
    "transform-class-properties"
  ]
}